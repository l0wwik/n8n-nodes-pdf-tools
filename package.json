{
  "name": "@l0wwik/n8n-nodes-split-pdf",
  "version": "1.3.0",
  "description": "A custom n8n node to extract specific pages from a PDF (Split PDF)",
  "keywords": [
    "n8n",
    "n8n-community-node",
    "pdf",
    "split",
    "n8n-node"
  ],
  "license": "MIT",
  "homepage": "https://github.com/l0wwik/n8n-nodes-split-pdf",
  "author": {
    "name": "l0wwik",
    "url": "https://github.com/l0wwik"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/l0wwik/n8n-nodes-split-pdf.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix"
  },
  "files": [
    "dist",
    "nodes",
    "README.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/SplitPdf/SplitPdf.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/form-data": "^2.2.1",
    "@types/node": "^22.13.13",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "pdf-lib": "^1.17.1"
  }
}
